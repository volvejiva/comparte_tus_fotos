{% extends 'baseTemplate.html.twig' %}

{% block content %}
        <div class="row">
            <div class="col-lg-12">
                <div class="text-center m-t-lg">
                    <h1>{{ "wellcome.title"|trans }}</h1>
                    <small>
                        {{ "wellcome.description"|trans }}
                    </small>
                    <h2><b>{{ "album.creation"|trans }}</b></h2>
                    
                    {{ form_start(form) }}
                        {{ form_widget(form) }}
                        <input type="submit" value="{{ 'form.button'|trans }}" label="Crear Album" />
                    {{ form_end(form) }}
                </div>
            </div>
        </div>
{% endblock %}
